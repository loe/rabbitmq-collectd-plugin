<LoadPlugin python>
    Globals true
</LoadPlugin>

<Plugin python>
    ModulePath "/etc/collectd/scripts/rabbitmq-collectd-plugin/"
    LogTraces true
    Import "rabbitmq_info"
    <Module rabbitmq_info>
       Vhost "/"
       #RmqcBin
       #PmapBin
       #PidFile
       #Verbose boolean
       #Vhost one two three
    </Module>
</Plugin>

